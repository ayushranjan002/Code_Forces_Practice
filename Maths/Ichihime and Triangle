#include <iostream>
using namespace std;

int main() {
    int t;
    cin >> t;  

    while (t--) {
        long long a, b, c, d;
        cin >> a >> b >> c >> d;

     
        long long x = b;
        long long y = c;
        long long z = c;

        cout << x << " " << y << " " << z << endl;
    }

    return 0;
}
