#include <bits/stdc++.h>
using namespace std;
int main() {
    int x = 0;
    int y = 0;
    //cout<<"enter"<<endl;
    for(int i = 0; i < 5; i++){
        //cout<<"row"<<i<<endl;
        for(int j = 0; j < 5; j++){
            int t;
            cin >> t;
            if(t == 1) {
                x = i;
                y = j;
            }
        }
    }
    int ans = abs(x - 2) + abs(y - 2);

    cout<<ans;
    return 0;
}
