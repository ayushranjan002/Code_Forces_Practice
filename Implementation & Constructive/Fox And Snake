#include <bits/stdc++.h>
using namespace std;

int main() {
    int n, m;
    cin >> n >> m;
    vector<vector<char>> v(n, vector<char>(m, '.'));
    bool row = true;

    for (int i = 0; i < n; i += 2) {
        for (int j = 0; j < m; j++)
            v[i][j] = '#';
        
        if (i + 1 < n) {
            if (row) {
                v[i + 1][m - 1] = '#';
                row = false;
            } else {
                v[i + 1][0] = '#';
                row = true;
            }
        }
    }

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++)
            cout << v[i][j];
        cout << endl;
    }
    return 0;
}
