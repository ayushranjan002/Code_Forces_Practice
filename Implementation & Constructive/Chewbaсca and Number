#include <bits/stdc++.h>
using namespace std;

int main() {
    long long num;
    cin >> num;
    string s = to_string(num);
    string ans = "";
    
    for (int i = 0; i < s.size(); i++) {
        int digit = s[i] - '0';
        int temp = 9 - digit;
        if (i == 0 && digit == 9) ans += '9';
        else ans += (char)('0' + min(digit, temp));
    }

    cout << ans;
    return 0;
}
